# DNSPod API Test Credentials
# Copy this file to `.env` and fill in your values
# The .env file is gitignored and safe for credentials

# API Credentials (from Tencent Cloud console)
DNSPOD_SECRET_ID=your_secret_id_here
DNSPOD_SECRET_KEY=your_secret_key_here

# User-Agent configuration (required by DNSPod API spec)
# These should identify YOUR program, not the library
DNSPOD_TEST_PROGRAM=libdns-rs
DNSPOD_TEST_VERSION=0.1.0
DNSPOD_TEST_EMAIL=your@email.com
